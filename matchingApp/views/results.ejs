<div>
	<% for(let i = 0; i < data.length; i++){ %>
	<div class="albumCard">
		<div class="albumImage" style="background-image: url('<%= data[i].Image %>')"></div>
		<div>
			<p><%= data[i].Title %></p>
			<p><%= data[i].Genre %> | <%= data[i].Year %></p>
		</div>

		<p><%= data[i].Artist %></p>
	</div>

	<% } %> <%- include('navigation') %>
</div>
